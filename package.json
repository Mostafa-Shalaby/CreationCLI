{
  "name": "creation-cli",
  "description": "A simple cli tool to save time in creating files for large projects",
  "version": "1.0.0",
  "main": "app.mjs",
  "scripts": {
    "start": "node app.mjs",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "npx eslint --fix .",
    "prettier": "npx prettier --check . ",
    "prettier:fix": "npx prettier --write . ",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mostafa-Shalaby/CreationCLI.git"
  },
  "keywords": [
    "CLI",
    "nodejs",
    "typescript"
  ],
  "author": "Mostafa Shalaby",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mostafa-Shalaby/CreationCLI/issues"
  },
  "homepage": "https://github.com/Mostafa-Shalaby/CreationCLI#readme",
  "devDependencies": {
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "jest": "^29.4.3",
    "prettier": "^2.8.4"
  }
}
